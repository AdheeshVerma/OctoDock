    "@types/express-rate-limit": "^6.0.2",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.6",
    "@types/morgan": "^1.9.10",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "animejs": "^4.1.0",
    "graphql": "^16.11.0",
